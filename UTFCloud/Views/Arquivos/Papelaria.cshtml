@model IEnumerable<Modelo.Arquivos>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Meus arquivos";
}
@if (Model != null && Model.Count() > 0)
{
    <div class="panel panel-default">
    <div class="panel-heading">
        <h4><b>Arquivos</b></h4>
    </div>

        <div class="panel-body">
            <div class="panel-body">
                @Html.Partial("~/Views/Arquivos/_PartialPapelariaContentPanel.cshtml")
            </div>
        </div>
    </div>
}
else
{
<div class="container text-center">
    <i class="glyphicon glyphicon-download" style="color:rgba(237, 236, 236, 0.50); font-size:300px"></i>
    <br /><h2 style="color:rgba(237, 236, 236, 0.50);">Busque seu arquivo e faça o download</h2>
</div>
}

@section ScriptPage {
    <script>
        $(document).ready(function () {
            $(".busca").css("display", "block");
        })

    </script>
}